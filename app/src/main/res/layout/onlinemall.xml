<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

     <!--   <ImageButton
            android:id="@+id/btn_rotate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:background="#00ffffff"
            android:gravity="center"
            android:src="@drawable/basketImage" />
-->
        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="온라인 몰"
            android:textColor="#000000"
            android:textSize="30sp"
            android:textStyle="bold" />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_marginTop="50dp"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingLeft="50dp"
                android:paddingStart="50dp">

                <ImageButton
                    android:id="@+id/emartButton"
                    android:layout_width="64dp"
                    android:layout_height="67dp"
                    android:elevation="30dp"
                    android:background="@drawable/mainbutton_state"
                    app:srcCompat="@drawable/ic_search_black_24dp"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="이마트몰"
                    android:textSize="15sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingRight="50dp">

                <ImageButton
                    android:id="@+id/lottemartButton"
                    android:layout_width="64dp"
                    android:layout_height="67dp"
                    android:elevation="30dp"
                    android:background="@drawable/mainbutton_state"
                    app:srcCompat="@drawable/ic_search_black_24dp"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="롯데마트"
                    android:textSize="15sp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingLeft="50dp"
                android:layout_weight="1"
                android:paddingStart="50dp">
                <ImageButton
                    android:id="@+id/homeplusButton"
                    android:layout_width="64dp"
                    android:layout_height="67dp"
                    android:elevation="30dp"
                    android:background="@drawable/mainbutton_state"
                    app:srcCompat="@drawable/ic_search_black_24dp"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/textView_dan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="홈플러스"
                    android:textSize="15sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingRight="50dp"
                android:layout_weight="1">
                <ImageButton
                    android:id="@+id/traders"
                    android:layout_width="64dp"
                    android:layout_height="67dp"
                    android:elevation="30dp"
                    android:background="@drawable/mainbutton_state"
                    app:srcCompat="@drawable/ic_search_black_24dp"
                    tools:ignore="VectorDrawableCompat"/>
                <TextView
                    android:id="@+id/textView_song"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="트레이더스"
                    android:textSize="15sp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:elevation="30dp"
                android:gravity="center"
                android:paddingLeft="50dp"
                android:paddingStart="50dp"
                android:layout_weight="1">
                <ImageButton
                    android:id="@+id/hanaromartButton"
                    android:layout_width="64dp"
                    android:layout_height="67dp"
                    android:elevation="30dp"
                    android:background="@drawable/mainbutton_state"
                    app:srcCompat="@drawable/ic_search_black_24dp"
                    tools:ignore="VectorDrawableCompat" />
                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="하나로마트"
                    android:textSize="15sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingRight="50dp"
                android:layout_weight="1">
                <ImageButton
                    android:id="@+id/costcoButton"
                    android:layout_width="64dp"
                    android:layout_height="67dp"
                    android:elevation="30dp"
                    android:background="@drawable/mainbutton_state"
                    app:srcCompat="@drawable/ic_search_black_24dp"
                    tools:ignore="VectorDrawableCompat" />
                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="코스트코"
                    android:textSize="15sp" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</FrameLayout>
